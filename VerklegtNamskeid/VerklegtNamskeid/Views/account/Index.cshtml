@model IEnumerable<VerklegtNamskeid.Models.UserAccount>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model._userName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model._userEmail)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item._userName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item._userEmail)
        </td>
        
    </tr>
}

</table>
